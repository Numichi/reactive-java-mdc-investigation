<Configuration packages="com.example.reactivejavamdc.log4j2">
    <Appenders>
        <Console name="ExampleLog">
<!--            <ExamplePlugin/>-->
            <PatternLayout
                pattern="%d{yyyy-MM-dd HH:mm:ss} %-5p %c{1}:%L - %m%n" />
        </Console>
    </Appenders>

    <Loggers>
        <AsyncRoot level="info">
           <AppenderRef ref="ExampleLog"/>
        </AsyncRoot>
    </Loggers>
</Configuration>